      window.onkeydown = (e) => {
          if (e.ctrlKey || e.altKey || e.shiftKey || e.metaKey) {
              return;
          }
          
          
          if (e.key == "ArrowRight") {
              window.location.href = 'values';
          }
          
      }
  </script>
