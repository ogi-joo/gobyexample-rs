      window.onkeydown = (e) => {
          if (e.ctrlKey || e.altKey || e.shiftKey) {
              return;
          }
          
          if (e.key == "ArrowLeft") {
              window.location.href = 'for';
          }
          
          
          if (e.key == "ArrowRight") {
              window.location.href = 'switch';
          }
          
      }
  </script>
