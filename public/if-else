      var codeLines = [];
      codeLines.push('');codeLines.push('package main\u000A');codeLines.push('import \"fmt\"\u000A');codeLines.push('func main() {\u000A');codeLines.push('    if 7%2 \u003D\u003D 0 {\u000A        fmt.Println(\"7 je paran\")\u000A    } else {\u000A        fmt.Println(\"7 je neparan\")\u000A    }\u000A');codeLines.push('    if 8%4 \u003D\u003D 0 {\u000A        fmt.Println(\"8 je deljivo sa 4\")\u000A    }\u000A');codeLines.push('    if 8%2 \u003D\u003D 0 || 7%2 \u003D\u003D 0 {\u000A        fmt.Println(\"jedan od brojeva 8 ili 7 je paran\")\u000A    }\u000A');codeLines.push('    if num :\u003D 9; num \u003C 0 {\u000A        fmt.Println(num, \"je negativan\")\u000A    } else if num \u003C 10 {\u000A        fmt.Println(num, \"ima jednu cifru\")\u000A    } else {\u000A        fmt.Println(num, \"ima više cifra\")\u000A    }\u000A}\u000A');codeLines.push('');codeLines.push('');codeLines.push('');
