      var codeLines = [];
      codeLines.push('');codeLines.push('package main\u000A');codeLines.push('import (\u000A    \"fmt\"\u000A    \"time\"\u000A)\u000A');codeLines.push('func main() {\u000A');codeLines.push('    i :\u003D 2\u000A    fmt.Print(i, \" se piše kao \")\u000A    switch i {\u000A    case 1:\u000A        fmt.Println(\"jedan\")\u000A    case 2:\u000A        fmt.Println(\"dva\")\u000A    case 3:\u000A        fmt.Println(\"tri\")\u000A    }\u000A');codeLines.push('    switch time.Now().Weekday() {\u000A    case time.Saturday, time.Sunday:\u000A        fmt.Println(\"Danas je vikend!\")\u000A    default:\u000A        fmt.Println(\"Danas je radni dan.\")\u000A    }\u000A');codeLines.push('    t :\u003D time.Now()\u000A    switch {\u000A    case t.Hour() \u003C 12:\u000A        fmt.Println(\"Prepodne je\")\u000A    default:\u000A        fmt.Println(\"Poslepodne je\")\u000A    }\u000A');codeLines.push('    whatAmI :\u003D func(i interface{}) {\u000A        switch t :\u003D i.(type) {\u000A        case bool:\u000A            fmt.Println(\"Ja sam bool\")\u000A        case int:\u000A            fmt.Println(\"Ja sam int\")\u000A        default:\u000A            fmt.Printf(\"Ne znam šta je %T\\n\", t)\u000A        }\u000A    }\u000A    whatAmI(true)\u000A    whatAmI(1)\u000A    whatAmI(\"hey\")\u000A}\u000A');codeLines.push('');
    </script>
