      window.onkeydown = (e) => {
          if (e.ctrlKey || e.altKey || e.shiftKey || e.metaKey) {
              return;
          }
          
          if (e.key == "ArrowLeft") {
              window.location.href = 'constants';
          }
          
          
          if (e.key == "ArrowRight") {
              window.location.href = 'if-else';
          }
          
      }
